   GetFileInfo(BaseInfo.ProjectByName[Self.ClassProject].Folder + BaseInfo.ProjectByName[Self.ClassProject].Name + ".#prj",Size, ADate, Attr);
   ДатаПроекта=CreateDate(Day(aDate),Mon(aDate),Год(aDate));
   Result = ДатаПроекта;
  end;

